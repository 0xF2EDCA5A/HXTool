{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "A simple bar chart with embedded data.",
  "autosize": {
    "type": "fit",
    "contains": "content"
  },
  "data": { "url": "/api/v1/vegalite_events_distribution"},
  "mark": "bar",
  "encoding": {
    "x": {"field": "source", "type": "ordinal", "axis": {"title": false}},
    "y": {"field": "count", "type": "quantitative"}
  }
}
