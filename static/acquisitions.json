{
	"agentinfo": {
		"description": "Collects basic information about the agents on host endpoints that does not appear in the sysinfo audit.",
		"platforms": ["win", "osx", "linux"],
		"parameters": false
	},
	"config": {
		"description": "The config audit collects key-value pairs from the agent configuration file or acquires the entire agent configuration file, depending on the format of the audit request",
		"platforms": ["win", "osx", "linux"],
		"parameters": [
			{"name": "format", "required": false, "repeatable": false, "type": "dropdown", "values": ["kvpairs", "acquisition"]}
		]
	},
	"configuration": {
		"description": "The configuration audit collects information about the configuration channel to which the agents should subscribe",
		"platforms": ["win", "osx", "linux"],
		"parameters": [
			{"name": "ConfigChannel", "required": true, "repeatable": false, "type": "text"}
		]
	},
	"cookiehistory": {
		"description": "The following input parameters are available for this audit. Some parameters are supported only in Windows environments.",
		"platforms": ["win", "osx"],
		"parameters": [
			{"name": "TargetBrowser", "required": false, "repeatable": true, "type": "dropdown", "values": ["Firefox", "Internet Explorer", "Chrome", "Safari"]},
			{"name": "PathToHistoryFiles", "required": false, "repeatable": false, "type": "text"},
			{"name": "PreventHibernation", "required": false, "repeatable": false, "type": "dropdown", "values": ["true", "false"]}
		]
	},
	"multifile-acquisition-api": {
		"description": "Collects multiple files from host endpoints based on filter criteria (such as path or filename) using system calls for file access.",
		"platforms": ["win", "osx", "linux"],
		"parameters": [
			{"name": "Path", "required": true, "repeatable": false, "type": "text"},
			{"name": "Path Regex", "required": false, "repeatable": false, "type": "textarea"},
			{"name": "Minimum Sizes", "required": false, "repeatable": false, "type": "text"},
			{"name": "Maximum Sizes", "required": false, "repeatable": false, "type": "text"},
			{"name": "Filter MD5", "required": false, "repeatable": false, "type": "text"},
			{"name": "Filter SHA1", "required": false, "repeatable": false, "type": "text"},
			{"name": "Filter SHA256", "required": false, "repeatable": false, "type": "text"},
			{"name": "Content Regex", "required": false, "repeatable": false, "type": "textarea"},
			{"name": "AND Operator", "required": false, "repeatable": false, "type": "text"},
			{"name": "PreventHibernation", "required": false, "repeatable": false, "type": "dropdown", "values": ["true", "false"]}
		]
	}
}