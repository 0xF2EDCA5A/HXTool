{
	"log_handlers": {
		"rotating_file_handler": {
			"file": "log/hxtool.log",
			"max_bytes": 50000,
			"backup_count": 5,
			"level": "info",
			"format": "[%(asctime)s] {%(module)s} {%(threadName)s} %(levelname)s - %(message)s"
		}
	},
	"network": {
		"ssl": "enabled",
		"port": 8080,
		"listen_address": "0.0.0.0",
		"session_timeout": 30
	},
	"ssl": {
		"cert": "hxtool.crt",
		"key": "hxtool.key"
	},
	"scheduler": {
		"thread_count" : null,
		"defer_interval" : 30
	},
	"apicache": {
		"enabled": true,
		"fetcher_interval": 15,
		"objects_per_poll": 3,
		"refresh_interval": 60
	},
	"headers": {},
	"cookies": {}
}
