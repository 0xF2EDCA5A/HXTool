{
   "log_handlers":{
	   "stream_handler":{
		  "level":"info",
		  "format":"[%(asctime)s] {%(threadName)s} %(levelname)s - %(message)s"
	   },
	   "rotating_file_handler":{
		  "file":"log/hxtool.log",
		  "max_bytes":50000,
		  "backup_count":5,
		  "level":"info",
          "format":"[%(asctime)s] {%(threadName)s} %(levelname)s - %(message)s"
	   }
	},
   "network":{  
      "ssl":"enabled",
      "port":8080,
      "listen_address":"0.0.0.0"
   },
   "ssl":{ 
      "cert":"hxtool.crt",
      "key":"hxtool.key"
   },
   "background_processor":{  
      "downloads_per_poll":500,
      "poll_interval":5,
      "stack_jobs_per_poll":500
   },
   "headers":{
   },
   "cookies":{
      "XFIRE_HMAC_SESSION":"<change me>"
   }
}
